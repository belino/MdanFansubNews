             x:Class="MDAN.Android.MainPage">


        <ListView x:Name="listRss">
            <ListView.ItemTemplate>
                <DataTemplate>

                    <TextCell x:Name="title" Text="{Binding Title}" ></TextCell>

                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>

</ContentPage>
